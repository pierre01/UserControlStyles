<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="UserControlStyles.UserControls.MyCustomControl">
    <VerticalStackLayout>
        <HorizontalStackLayout>
            <Rectangle x:Name="UpRectangle"
                WidthRequest="50"
                HeightRequest="50"
                Fill="LightBlue"
                Stroke="DarkBlue"
                StrokeThickness="2" />
            <Label Text="Empty" Margin="16,0,0,0" x:Name="UpTextLabel" />
        </HorizontalStackLayout>
        <HorizontalStackLayout Margin="0,16,0,0">
            <Rectangle
                WidthRequest="50"
                HeightRequest="50"
                Fill="LightBlue"
                Stroke="DarkBlue"
                StrokeThickness="2" />
            <Label Text="Empty" Margin="16,0,0,0" />
        </HorizontalStackLayout>
    </VerticalStackLayout>
</ContentView>
